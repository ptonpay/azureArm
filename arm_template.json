                "typeProperties": {
                    "sources": [
                        {
                            "dataset": {
                                "referenceName": "InputDataSet",
                                "type": "DatasetReference"
                            },
                            "name": "source1",
                            "typeProperties": {}
                        }
                    ],
                    "sinks": [
                        {
                            "dataset": {
                                "referenceName": "Outputdataset",
                                "type": "DatasetReference"
                            },
                            "name": "sink1"
                        }
                    ],
                    "transformations": [
                        {
                            "name": "Select1"
                        },
                        {
                            "name": "Aggregate1"
                        }
                    ],
                    "script": "\n\nsource(output(\n\t\tcarat as string,\n\t\tcut as string,\n\t\tcolor as string,\n\t\tclarity as string,\n\t\tdepth as string,\n\t\ttable as string,\n\t\tprice as string,\n\t\tx as string,\n\t\ty as string,\n\t\tz as string\n\t),\n\tallowSchemaDrift: true,\n\tvalidateSchema: true,\n\trowUrlColumn: 'source_file',\n\twildcardPaths:['diamond*']) ~> source1\nsource1 select(mapColumn(\n\t\tcarat_f = carat,\n\t\tcut_f = cut,\n\t\tcolor_f = color,\n\t\tclarity_f = clarity\n\t),\n\tskipDuplicateMapInputs: true,\n\tskipDuplicateMapOutputs: true) ~> Select1\nSelect1 aggregate(groupBy(cut_f),\n\tcarat_f = sum(1)) ~> Aggregate1\nAggregate1 sink(input(\n\t\tColumn_1 as string,\n\t\tColumn_2 as string,\n\t\tColumn_3 as string,\n\t\tColumn_4 as string,\n\t\tColumn_5 as string,\n\t\tColumn_6 as string,\n\t\tColumn_7 as string,\n\t\tColumn_8 as string,\n\t\tColumn_9 as string,\n\t\tColumn_10 as string\n\t),\n\tallowSchemaDrift: true,\n\tvalidateSchema: false) ~> sink1"
                }
            },
